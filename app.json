{
  "expo": {
    "name": "Police App",
    "icon": "./assets/icon.png",
    "version": "1.0.0",
    "slug": "sih_police-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "scheme": "myapp",
    "ios":{
      "bundleIdentifier": "com.sih.sample"
    },
    "android": {
      "package": "com.sih.sample",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{
            "scheme": "your-scheme"
          }],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
